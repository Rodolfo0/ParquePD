@model ParqueProcesoDesarrollo.Web.Data.Entities.User
@{
    ViewData["Title"] = "Detalles";
}
<h1>Detalles</h1>

<hr />
<div class="row">
    <div class="col-md-4">
        <dl class="row">
            <dt class="col-sm-6">
                @Html.DisplayNameFor(model => model.Email)
            </dt>

            <dd class="col-sm-6">
                @Html.DisplayFor(model => model.FullName)
            </dd>

            <dt class="col-sm-6">
                @Html.DisplayNameFor(model => model.PhoneNumber)
            </dt>
            <dd class="col-sm-6">
                @Html.DisplayFor(model => model.RFC)
            </dd>

            <dt class="col-sm-6">
                @Html.DisplayNameFor(model => model.Role.Name)
            </dt>
            <dd class="col-sm-6">
                @Html.DisplayFor(model => model.Salary)
            </dd>
        </dl>

    </div>
</div>

<div>
    <a asp-action="Index" class="button"><i class="bi bi-arrow-return-left"></i></a>
</div>


